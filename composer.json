{
  "name": "demorfi/digua",
  "description": "PHP Micro Framework",
  "keywords": [
    "framework",
    "micro",
    "digua"
  ],
  "type": "framework",
  "license": "MIT",
  "authors": [
    {
      "name": "demorfi",
      "email": "demorfi@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-shmop": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.26"
  },
  "autoload": {
    "psr-4": {
      "Digua\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "stable"
}